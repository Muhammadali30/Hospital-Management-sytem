<Page x:Class="Final_Project.Forms.Laboratory.LabSidebarPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Final_Project.Forms.Laboratory"
      mc:Ignorable="d" 
        d:DesignHeight="300" d:DesignWidth="190"
  Background="#1E929B"
      Title="LabSidebarPage">

    <StackPanel>
        <Button   Style="{StaticResource editbutton}" Content="DashBoard" FontSize="16" FontWeight="SemiBold" Margin="10" Click="DashBoardPageButton" />
        <Button   Style="{StaticResource editbutton}" Content="Lab Invoice"  FontSize="16" Margin="10 5" FontWeight="semibold" Click="OpenLabInvoicePage" />
        <Button   Style="{StaticResource editbutton}" Content="Lab Reports"  FontSize="16" Margin="10 5" FontWeight="semibold" Click="OpenLabReportPage"/>
        <Button   Style="{StaticResource editbutton}" Content="Lab Templates"  FontSize="16" Margin="10 5" FontWeight="semibold" Click="OpenLabTemplatesPage" />
        <Button   Style="{StaticResource editbutton}" Content="Lab Department"  FontSize="16" Margin="10 5" FontWeight="semibold" Click="OpenLabDepartmentPage"  />
    </StackPanel>
    </Page>